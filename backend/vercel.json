{
    "version": 2,
    "builds": [
      { "src": "../frontend/build/**", "use": "@vercel/static" },
      { "src": ".", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/server.js" },
      { "src": ".*", "dest": "../frontend/build/index.html" }
    ]
  }
  